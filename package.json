{
  "name": "crypto-screener",
  "version": "2.0.0",
  "description": "Crypto Screener с чистой архитектурой и DDD",
  "main": "src/app.js",
  "scripts": {
    "main": "node scripts/virtual-trading-system.js",
    "start": "node src/app.js",
    "dev": "set NODE_ENV=development&& node src/app.js",
    "cli": "node src/cli.js",
    "summary": "node src/cli.js summary",
    "rising": "node src/cli.js rising",
    "falling": "node src/cli.js falling",
    "test": "node src/cli.js test",
    "help": "node src/cli.js help",
    "filter:marketcap": "node scripts/filter-by-market-cap-compact.js",
    "save:coins": "node scripts/save-filtered-coins.js",
    "save:extended": "node scripts/save-extended-coins.js",
    "filter:binance-pairs": "node scripts/filter-binance-trading-pairs.js",
    "filter:binance-only": "node scripts/filter-binance-only.js",
    "monitor:anomalies": "node scripts/anomaly-monitor.js",
    "monitor:signals": "node scripts/trade-signals-only.js",
    "monitor:virtual": "node scripts/virtual-trading-system.js",
    "monitor:websocket": "node scripts/start-websocket-system.js",
    "monitor:full": "node scripts/start-full-system.js",
    "monitor:websocket-daemon": "node scripts/start-websocket-daemon.js",
    "stop:websocket": "node scripts/stop-websocket-daemon.js",
    "logs:websocket": "node scripts/view-websocket-logs.js",
    "stats:view": "node scripts/view-statistics.js",
    "stats:extended": "node scripts/view-extended-statistics.js",
    "test:websocket-system": "node scripts/test-websocket-system.js",
    "status:view": "node scripts/view-system-status.js",
    "filter:binance": "node scripts/filter-binance-coins.js",
    "git:stage-trading": "node scripts/git-stage-trading-files.js stage",
    "git:check-trading": "node scripts/git-stage-trading-files.js check",
    "git:auto-trading": "node scripts/git-stage-trading-files.js auto",
    "status:send": "node scripts/send-system-status.js send",
    "status:preview": "node scripts/send-system-status.js preview",
    "clear:data": "del data\\active-trades.json data\\pending-anomalies.json data\\trade-history.json data\\trading-statistics.json"
